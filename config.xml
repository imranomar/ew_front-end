<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.eazywashgo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Eazywash</name>
    <description>
        Eazywasho laundary app
    </description>
    <author email="dev@eazywash.org" href="http://cordova.io">
        Eazywash Team
    </author>
    <preference name="loadUrlTimeoutValue" value="700000" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="false" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/Icon-36.png" />
        <icon density="mdpi" src="res/icon/android/Icon-48.png" />
        <icon density="hdpi" src="res/icon/android/Icon-72.png" />
        <icon density="xhdpi" src="res/icon/android/Icon-96.png" />
        <icon density="xxhdpi" src="res/icon/android/Icon-144.png" />
        <icon density="xxxhdpi" src="res/icon/android/Icon-192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-fcm" spec="^2.1.2" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <engine name="android" spec="^7.1.2" />
    <engine name="ios" spec="^4.5.5" />
</widget>
